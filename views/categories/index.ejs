<!DOCTYPE html>
<html>

<body>

<main>
    <h1>Categories</h1>
    <% categories.forEach(function(category) { %>
      <div>
        <p><%= category.id %>:
          <a href="<%= `/categories/${category.name}` %>">
            <%= category.name %>
          </a>
        </p>
      </div>
    <% }); %>
</main>
<a href="<%= '/' %>">Back to Home</a>
</body>
</html>
