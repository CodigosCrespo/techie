<!DOCTYPE html>
<html>
<% include ../partials/head.ejs %>
<body>
<% include ../partials/headercat.ejs %>
<main>
    <h1>Categories</h1>
    <% categories.forEach(function(category) { %>
      <div>
        <p><%= category.id %>:
          <a href="<%= `/categories/${category.name}` %>">
            <%= category.name.replace(/_/g, ' ') %>
          </a>
        </p>
      </div>
    <% }); %>
</main>
<a href="<%= '/' %>">Back to Home</a>
</body>
</html>
